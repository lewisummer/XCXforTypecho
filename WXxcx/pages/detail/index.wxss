/**基础风格样式**/
@import '/towxml/style/main.wxss';


/**如果页面有动态主题切换，则需要将使用到的样式全部引入**/

/**主题配色（浅色样式）**/
@import '/towxml/style/theme/light.wxss';

/**主题配色（深色样式）**/
@import '/towxml/style/theme/dark.wxss';

.floating {
	width: 90rpx;
	height: 90rpx;
	display: flex;
	position: fixed;
	right: 30rpx;
	bottom: 30rpx;
	justify-content: center;
	align-items: center;
}

.floating image {
	width: 50rpx;
	height: 50rpx;
}